{
    "cSpell.words": [
        "pygptlink",
        "tiktoken"
    ],
    "files.exclude": {
        ".venv*": true,
        ".mypy*": true,
        ".coverage": true,
        "**/__pycache__": true,
        "build": true,
        ".idea": true,
        "*.egg-info": true,
    },
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./test",
        "-p",
        "test_*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "python.analysis.typeCheckingMode": "strict"
}